@model SettingUpdateDto


<div class="form-container container shadow">
    <div>
        <h1>Update Setting</h1>
        <div class="back-to-list">
            <a class="btn btn-success " title="back to list" asp-action="Index"><i class="fa-solid fa-list"></i></a>
        </div>
    </div>
    <form method="post" enctype="multipart/form-data">
        <div class="mb-3">
            <label asp-for="Key" class="form-label">Key</label>
            <input asp-for="Key" class="form-control">
            <span asp-validation-for="Key"></span>
        </div>
        <div class="mb-3">
            <label asp-for="Value" class="form-label">Value</label>
            <input asp-for="Value" class="form-control">
            <span asp-validation-for="Value"></span>
        </div>


        <button type="submit" class="btn btn-primary">Update</button>
        <a asp-action="Index" asp-controller="setting" class="btn btn-info">Back To Home</a>
    </form>
</div>
