@model List<SubInstructionLayoutDto>
<section class="mf-breadcrumb">
    <div class="container">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a asp-action="index" asp-controller="home">Home</a></li>
                <li class="breadcrumb-item active" aria-current="page">Referendum</li>
                <li class="breadcrumb-item active" aria-current="page">Instructions</li>

            </ol>
        </nav>
    </div>
</section>
<section class="main-content">
    <div class="container">
        <div class="row">

            <div class="col-lg-12">

                <div class="page-post">
                    <h3>Instructions</h3>
                    <div class="page-post-items">
                        @foreach (var subInstruction in Model)
                        {
                            <div class="page-post-item">
                                <p class="post-title">
                                    <a href="~/assets/pdf/instruction/@subInstruction.Url" class="link-more"> @subInstruction.Title</a>
                                </p>
                            </div>

                        }


                        <br><br>
                    </div>
                </div>

            </div>

        </div>
    </div>
</section>
