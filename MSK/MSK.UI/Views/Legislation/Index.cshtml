@model List<LegislationLayoutDto>
<section class="mf-breadcrumb">
    <div class="container">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a asp-controller="home" asp-action="index">Home</a></li>

                <li class="breadcrumb-item active" aria-current="page">Legislation</li>
            </ol>
        </nav>
    </div>
</section>

<section class="main-content">
    <div class="container">
        <div class="row">

            <div class="col-lg-12">

                <div class="page-inner">
                    <h3>Legislation</h3>
                    <div class="page-inner-item">
                        <table class="table table-hover table-striped shadow">
                            <tbody>
                                <tr>
                                    <th>Document</th>
                                    <th>Download</th>
                                </tr>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>&nbsp;&nbsp; <strong>@item.Name</strong></td>
                                        <td><a href="~/assets/pdf/legislation/@item.PdfUrl" target="_blank" rel="noreferrer noopener"><img style="display:block;margin-left:auto;margin-right:auto;" src="~/assets/img/pdf.png" alt=""></a></td>
                                    </tr>

                                }
                            </tbody>
                        </table>
                        <div>
                            <br>
                            <a href="/en/decisions">Decisions</a>
                            <br>
                            <br>
                            <a href="/en/instructions">Instructions</a>
                            <br>
                            <br>
                            <a href="/en/pages/view/esasname">Regulations</a>
                        </div>
                    </div>
                </div>

            </div>

        </div>
    </div>
</section>