@model AccredationLayoutDto
<section class="mf-breadcrumb">
    <div class="container">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a  asp-controller="home" asp-action="index">Home</a></li>
             
                <li class="breadcrumb-item active" aria-current="page">Accredation</li>
            </ol>
        </nav>
    </div>
</section>

<section class="main-content">
    <div class="container">
        <div class="row">

            <div class="col-lg-12">

                <div class="page-inner">
                    <div class="page-inner container">
                        <h3>Accreditation</h3>
                        <div class="page-inner-item">
                            <p><strong>@Model.Name</strong></p>
                            <table class="table table-hover table-striped shadow">
                                <tbody>
                                    <tr>
                                        <th>Document</th>
                                        <th>Download (pdf, word)</th>
                                    </tr>
                                    <tr>
                                        <td>&nbsp;&nbsp;&nbsp; <strong>Application form for International Observers (EN)</strong></td>
                                        <td><a href="~/assets/pdf/accredation/@Model.PDFUrlEn" target="_blank" rel="noreferrer noopener"><img style="display:block;margin-left:auto;margin-right:auto;" src="~/assets/img/pdf.png" alt=""></a></td>
                                    </tr>
                                    <tr>
                                        <td>&nbsp;&nbsp;&nbsp; <strong>Application form for International Observers (RU)</strong></td>
                                        <td><a href="~/assets/pdf/accredation/@Model.PDFUrlRu" target="_blank" rel="noreferrer noopener"><img style="display:block;margin-left:auto;margin-right:auto;" src="~/assets/img/pdf.png" alt=""></a></td>
                                    </tr>
                                    <tr>
                                        <td>&nbsp;&nbsp;&nbsp; <strong>Application form for Interpreters (AZ)</strong></td>
                                        <td><a href="~/assets/pdf/accredation/@Model.PDFUrlAz" target="_blank" rel="noreferrer noopener"><img style="display:block;margin-left:auto;margin-right:auto;" src="~/assets/img/pdf.png" alt=""></a></td>
                                    </tr>
                                </tbody>
                            </table>
                            <div>&nbsp;</div>
                        </div>
                    </div>
                </div>

            </div>

        </div>
    </div>
</section>
