<body>

    <section class="mf-elections">
        <div class="container">
            <div class="row">
                <div class="col-md-7 d-flex">
                    <div class="d-flex align-items-center">
                        <div class="header-message">
                            <h4>FEBRUARY 7, 2024</h4>
                            <h5>EARLY PRESIDENTIAL ELECTIONS</h5>
                            <h4>IN THE REPUBLIC OF AZERBAIJAN</h4>
                        </div>
                    </div>
                </div>
                <div class="col-md-5 mf-count-down d-flex justify-content-lg-end justify-content-md-start">
                    <div id="countdown" class="d-flex align-items-center">
                        <ul>
                            <li><span id="days">4</span>Day</li>
                            <li><span id="hours">21</span>Hour</li>
                            <li><span id="minutes">41</span>Minute</li>
                            <li><span id="seconds">52</span>Second</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- header -->
    <section id="navLogo" class="mf-header d-none d-lg-block">
        <div class="container">
            <!-- logo -->
            <div class="row">
                <div class="col-lg-8 col-sm-7 col-xs-12">
                    <a href="#navLogo" class="mf-logo d-flex align-items-center">
                        <span class="mf-logo-img"><img src="~/assets/img/logo.png" alt="logo"></span>
                        <span class="mf-big-title"><strong>Republic of Azerbaijan <br>Central Election Commission</strong></span>
                    </a>
                </div>
                <div class="col-lg-4 col-sm-5 col-xs-12 d-flex flex-column align-self-center">
                    <div class="d-flex flex-row justify-content-end mb-4">
                        <ul class="mf-social-icons d-flex flex-row align-items-center">
                            <li>
                                <a class="fb" target="_blank" href="https://www.facebook.com/msk.gov.az">

                                    <i class="fa-brands fa-square-facebook"></i>
                                </a>
                            </li>
                            <li>
                                <a class="tw" target="_blank" href="https://twitter.com/AzRes_MSK">
                                    <i class="fa-brands fa-square-twitter"></i>
                                </a>
                            </li>
                            <li>
                                <a class="yt" target="_blank" href="https://www.youtube.com/videos">
                                    <i class="fa-brands fa-square-youtube"></i>
                                </a>
                            </li>
                            <li>
                                <a class="ins" target="_blank" href="https://www.instagram.com/">
                                    <i class="fa-brands fa-square-instagram"></i>
                                </a>
                            </li>
                        </ul>
                        @*  <ul class="mf-langs d-flex flex-row align-items-center">
                        <li><a href="/" class="nav-link">AZE</a></li>
                        <li><a href="/en/" class="nav-link">ENG</a></li>
                        </ul>*@
                        <button class="btn btn-warning  d-none" id="voiceStoper"> Stop Voice Helper</button>
                    </div>
                    <div class="mf-search d-flex flex-row justify-content-end">
                        <div id="searchContainer" class="layout__input d-flex align-items-center">

                            <form id="form-search" asp-action="search" class="d-flex align-items-center mt-2" asp-controller="home" method="get">
                                <input type="text" id="search-input" name="query" placeholder="Search" for="search" autocomplete="off" />

                            </form>


                        </div>

                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- nav -->
    <section class="mf-nav">
        <div class="container">
            <nav class="navbar navbar-expand-lg navbar-light">
                <a class="mf-logo navbar-brand d-flex align-items-center d-lg-none" href="/en">
                    <span class="mf-logo-img"><img src="~/assets/img/logo.png" alt="logo"></span>
                    <span class="mf-big-title"><strong>Republic of Azerbaijan <br>Central Election Commission</strong></span>
                </a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarCollapse">
                    <ul class="navbar-nav">
                        <li class="nav-item dropdown">
                            <a class=" dropdown-toggle nav-link" asp-controller="azerbaijan" asp-action="hac" data-bs-toggle="dropdown" aria-expanded="false">
                                Azerbaijan
                            </a>

                            <ul class="dropdown-menu">
                                <li><a class="dropdown-item" asp-controller="azerbaijan" asp-action="nh">National Attributes</a></li>
                                <li><a class="dropdown-item" asp-controller="azerbaijan" asp-action="hac">History and Culture</a></li>

                            </ul>
                        </li>

                        <li class="nav-item">
                            <a asp-action="index" asp-controller="accredation" class="nav-link">Accreditation</a>
                        </li>
                        <li class="nav-item">
                            <a asp-action="index" asp-controller="legislation" class="nav-link">Legislation</a>
                        </li>
                        <li class="nav-item">
                            <a asp-action="index" asp-controller="referendum" class="nav-link">Referendum</a>
                        </li>
                        <li class="nav-item dropdown">

                            <a class=" dropdown-toggle nav-link" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">Elections</a>
                            <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                                <li>
                                    <a href="/en/elections/pages/elections-milli-majlis" class="dropdown-item">Elections to the Milli Majlis</a>
                                </li>
                                <li>
                                    <a href="/en/elections/pages/presidential-elections" class="dropdown-item">Presidential elections</a>
                                </li>
                                <li>
                                    <a href="/en/elections/pages/municipal-elections" class="dropdown-item">Municipal elections</a>
                                </li>
                            </ul>
                        </li>

                        <li class="nav-item">
                            <a asp-action="contact" asp-controller="home" class="nav-link">Contact</a>
                        </li>
                        <li class="nav-item">
                            <a href="/aisupport/index/#ai-input-form" class="nav-link">AI Support</a>
                        </li>
                        @if (User.Identity.IsAuthenticated)
                        {
                            <li class="nav-item">
                                <a asp-action="logout" asp-controller="account" class="nav-link">Logout</a>
                            </li>
                        }
                        else
                        {
                            <li class="nav-item">
                                <a asp-action="register" asp-controller="account" class="nav-link">Sign Up</a>
                            </li>
                            <li class="nav-item">
                                <a asp-action="login" asp-controller="account" class="nav-link">Sign In</a>
                            </li>
                        }
                    </ul>

                    <ul class="mf-mobile-langs d-flex d-lg-none flex-row align-items-center">
                        <li><a href="/az/" class="nav-link">AZE</a></li>
                        <li><a href="/en/" class="nav-link">ENG</a></li>
                    </ul>
                </div>
            </nav>
        </div>
    </section>


    <main role="main" class="body-main">
        <div class="body container-body">
            @RenderBody()

        </div>
    </main>
